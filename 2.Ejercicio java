//Estructura condicional
import java.util.Scanner;
class Main{
  public static void main(String[]args) {
    Scanner leer=new Scanner(System.in);
    int a=0 , b=0;
    System.out.print("1. Ingrese el primer numero entero: ");
    a=leer.nextInt();
    System.out.print("2.Ingrese el segundo numero entero: ");
    b=leer.nextInt();
    if (a==b){
      System.out.print("Son iguales : "+a+" y "+b);
    }else if (a<b) {
      System.out.print("El mayor es : "+b);
    }else{
    System.out.print("El mayor es : "+a);
    }
  }

}
